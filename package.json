{
  "name": "EBMK",
  "version": "3.0.3",
  "main": "main.js",
  "description": "electron-updater example project",
  "author": "CutLine",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "devDependencies": {
    "electron": "^7.1.2",
    "electron-builder": "^23.0.2"
  },
  "dependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "fs.promises": "^0.1.2"
  },
  "build": {
    "mac": {
      "category": "your.app.category.type"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "category": "your.app.category.type",
      "target": "AppImage"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KIM-JS-95/PillInfoservice_FE.git"
  },
